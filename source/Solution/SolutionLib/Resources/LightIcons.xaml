<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >

    <!-- Setup TreeView Icons as StaticResource -->
    <BitmapImage x:Key="FileIcon" UriSource="/SolutionLib;component/Resources/Files/Light/Document_16x.png" />

    <BitmapImage x:Key="FolderIcon" UriSource="/SolutionLib;component/Resources/Folders/Light/Folder_32x.png" DecodePixelHeight="16" DecodePixelWidth="16" />
