    <BitmapImage x:Key="ProjectIcon" UriSource="/SolutionLib;component/Resources/Projects/Light/Application_16x.png" />

    <BitmapImage x:Key="SolutionIcon" UriSource="/SolutionLib;component/Resources/Solutions/Light/AppFlyout_16x.png" />
